{
  "presets": [
    [
      "@babel/preset-env"
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    ["@babel/plugin-proposal-class-properties", { "loose": false }]
  ],
  "env": {
    "commonjs": {
      "presets": [
        [
          "@babel/preset-env", {
            "useBuiltIns": false
          }
        ]
      ]
    },
    "es": {
      "presets": [
        [
          "@babel/preset-env", {
            "useBuiltIns": false,
            "modules": false
          }
        ]
      ]
    }
  }
}